
<div class="page-container">
	<%= form_for(@todo) do |f| %>

	  <%= f.label :title %>
	  <%= f.text_field :title %>

	  <%= f.label :body %>
	  <%= f.text_field :body %>

	  <%= f.label :priority %>
	  <%= f.text_field :priority %>

	  <%= f.label :complete %>
	  <%= f.text_field :complete %>

	  <%= f.submit %>

	<% end %>
</div>